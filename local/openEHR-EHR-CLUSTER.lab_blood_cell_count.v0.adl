				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Eosinophil count
				value matches {
					C_DV_QUANTITY <
						
						list = <
							["1"] = <
								units = <"10*9/L">
							>
							["2"] = <
								units = <"10*3/uL">
							>
							["3"] = <
								units = <"1/[HPF]">
							>
						>
					>
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- Basophil count
				value matches {
					C_DV_QUANTITY <
						
						list = <
							["1"] = <
								units = <"10*9/L">
							>
							["2"] = <
								units = <"10*3/uL">
							>
							["3"] = <
								units = <"1/[HPF]">
							>
						>
					>
				}
			}
			ELEMENT[at0011] occurrences matches {0..1} matches {    -- Blast count
				value matches {
					C_DV_QUANTITY <
						
